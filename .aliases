alias zshconfig="vim ~/.zshrc"
alias vimconfig='vim ~/.vimrc'

alias ghidra='/opt/ghidra/ghidraRun && exit' 

alias ve='python3 -m venv ./venv'
alias va='source ./venv/bin/activate'
alias tmux='tmux -2'
alias gdb='gdb -q'
alias ipython='python3 -m IPython'
alias hc='hashcat'
alias msf='msfconsole -q'
alias hashid='python3 ~/tools/hash-id.py'
alias sublister='sublist3r'
alias jetbrains='/opt/jetbrains/jetbrains-toolbox && disown'
alias tvpn='sudo tmux new -d -s vpn openvpn /opt/thm.ovpn'

alias lazygit='git add . && git commit -m "." && git push'
alias xclip="xclip -selection c"
alias sound="alsamixer"
